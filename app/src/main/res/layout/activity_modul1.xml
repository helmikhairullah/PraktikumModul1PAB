<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.helmikhairullah.modul1.modul1">

    <Button
        android:id="@+id/eatbus1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="57dp"
        android:layout_toLeftOf="@+id/textView10"
        android:layout_toStartOf="@+id/textView10"
        android:background="@android:color/holo_blue_light"
        android:onClick="eatbus2"
        android:text="Eatbus"
        tools:layout_editor_absoluteX="41dp"
        tools:layout_editor_absoluteY="384dp" />

    <Button
        android:id="@+id/abnormal1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/eatbus1"
        android:layout_alignBottom="@+id/eatbus1"
        android:layout_toEndOf="@+id/textnama"
        android:layout_toRightOf="@+id/textnama"
        android:background="@android:color/holo_blue_light"
        android:onClick="abnormal2"
        android:text="Abnormal"
        tools:layout_editor_absoluteX="211dp"
        tools:layout_editor_absoluteY="384dp" />

    <EditText
        android:id="@+id/textporsi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textnama"
        android:layout_alignStart="@+id/textnama"
        android:layout_below="@+id/textView10"
        android:layout_marginTop="103dp"
        android:ems="10"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="277dp" />

    <EditText
        android:id="@+id/textnama"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/eatbus1"
        android:layout_alignStart="@+id/eatbus1"
        android:layout_below="@+id/textporsi"
        android:layout_marginTop="88dp"
        android:ems="10"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="186dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="11dp"
        android:text="Makan Malam"
        android:textSize="60px"
        tools:layout_editor_absoluteX="98dp"
        tools:layout_editor_absoluteY="16dp" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="56dp"
        android:text="Dengan Milea!"
        tools:layout_editor_absoluteX="149dp"
        tools:layout_editor_absoluteY="91dp" />

</RelativeLayout>